{
  "name": "openingithub",
  "displayName": "OpenInGitHub",
  "description": "Opens the GitHub URL for a selected file",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "openingithub.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "openingithub.openInGitHub",
        "title": "Open in GitHub"
      },
      {
        "command": "openingithub.openInGitHubAtCommit",
        "title": "Open in GitHub at Current Commit"
      },
      {
        "command": "openingithub.openInGitHubAtCommitAndLine",
        "title": "Open in GitHub at Current Commit and Line"
      },
      {
        "command": "openingithub.openJDKTip",
        "title": "Open in GitHub on Tip"
      },
      {
        "command": "openingithub.openJDK25u",
        "title": "Open in GitHub on jdk25u"
      },
      {
        "command": "openingithub.openJDK24u",
        "title": "Open in GitHub on jdk24u"
      },
      {
        "command": "openingithub.openJDK21u",
        "title": "Open in GitHub on jdk21u"
      },
      {
        "command": "openingithub.openJDK17u",
        "title": "Open in GitHub on jdk17u"
      },
      {
        "command": "openingithub.openJDK11u",
        "title": "Open in GitHub on jdk11u"
      }
    ],
    "submenus": [
      {
        "id": "openingithub.openjdk",
        "label": "Open in GitHub (OpenJDK)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "!explorerResourceIsFolder",
          "command": "openingithub.openInGitHub",
          "group": "navigation"
        },
        {
          "when": "!explorerResourceIsFolder",
          "command": "openingithub.openInGitHubAtCommit",
          "group": "navigation"
        },
        {
          "when": "!explorerResourceIsFolder",
          "command": "openingithub.openInGitHubAtCommitAndLine",
          "group": "navigation"
        },
        {
          "when": "!explorerResourceIsFolder",
          "submenu": "openingithub.openjdk",
          "group": "1_openjdk"
        }
      ],
      "editor/context": [
        {
          "command": "openingithub.openInGitHub",
          "group": "navigation"
        },
        {
          "command": "openingithub.openInGitHubAtCommit",
          "group": "navigation"
        },
        {
          "command": "openingithub.openInGitHubAtCommitAndLine",
          "group": "navigation"
        },
        {
          "submenu": "openingithub.openjdk",
          "group": "1_openjdk"
        }
      ],
      "openingithub.openjdk": [
        {
          "command": "openingithub.openJDKTip"
        },
        {
          "command": "openingithub.openJDK25u"
        },
        {
          "command": "openingithub.openJDK24u"
        },
        {
          "command": "openingithub.openJDK21u"
        },
        {
          "command": "openingithub.openJDK17u"
        },
        {
          "command": "openingithub.openJDK11u"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
